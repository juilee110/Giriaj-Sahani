<script>
	(function(w,d,$){
		$(document).ready(function(){
			/* ---------------------------------------------------------------------- */
			/*	Contact Map
			/* ---------------------------------------------------------------------- */
			var contact = {"lat":"41.8744661", "lon":"-87.6614312", "maker":"http://qkthemes-demo.com/wordpress/industrial/wp-content/uploads/2015/07/marker.png"}; //Change a map coordinate here!

			try {
				var mapContainer = $('.map');
				mapContainer.gmap3({
					action: 'addMarker',
					marker:{
						options:{
							icon : new google.maps.MarkerImage( contact.maker )
						}
					},
					latLng: [contact.lat, contact.lon],
					map:{
						center: [contact.lat, contact.lon],
						zoom: 15
						},
					},
					{action: 'setOptions', args:[{scrollwheel:false}]}
				);
			} catch(err) {

			}
				});
	}(window, document, window.jQuery))
</script>
